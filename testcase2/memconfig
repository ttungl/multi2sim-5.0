;; 4 Memory Banks.

[Module mod-mm-0]
Type = MainMemory
BlockSize = 64
Latency = 100
Ports = 1
HighNetwork = net-l2-mm
HighNetworkNode = n12
AddressRange = ADDR DIV 64 MOD 4 EQ 0

[Module mod-mm-1]
Type = MainMemory
BlockSize = 64
Latency = 100
Ports = 1
HighNetwork = net-l2-mm
HighNetworkNode = n13
AddressRange = ADDR DIV 64 MOD 4 EQ 1

